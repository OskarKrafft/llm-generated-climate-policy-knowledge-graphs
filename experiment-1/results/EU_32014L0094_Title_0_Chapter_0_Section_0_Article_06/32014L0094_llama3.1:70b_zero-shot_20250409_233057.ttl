@prefix polianna: <https://ontology.egregium.com/polianna/> .

<https://ontology.egregium.com/polianna/resource/article_1>
    a            polianna:Article ;
    dc:title     "Natural Gas Refuelling Infrastructure" ;
    skos:prefLabel "Article 6 of Directive (EU) No 994/2010" ;
    dc:description """
        Article 6 of the EU Directive concerning measures to safeguard security of gas supply.
    """ .

<https://ontology.egregium.com/polianna/resource/article_1>
    polianna:hasArticleSection <https://ontology.egregium.com/polianna/resource/section_1> .

<https://ontology.egregium.com/polianna/resource/section_1>
    a            polianna:Section ;
    dc:title     "Natural Gas Refuelling Infrastructure" ;
    skos:prefLabel "Article 6, Section 1 of Directive (EU) No 994/2010" ;
    dc:description """
        Each Member State shall ensure that the minimum number and capacity of natural gas refuelling points accessible to the public are put in place.
    """ .

<https://ontology.egregium.com/polianna/resource/article_1>
    polianna:hasArticleSection <https://ontology.egregium.com/polianna/resource/section_2> .

<https://ontology.egregium.com/polianna/resource/section_2>
    a            polianna:Section ;
    dc:title     "Technical Specifications for Refuelling Points" ;
    skos:prefLabel "Article 6, Section 10 of Directive (EU) No 994/2010" ;
    dc:description """
        The Commission shall be empowered to adopt delegated acts to require compliance of the infrastructures with technical specifications.
    """ .

