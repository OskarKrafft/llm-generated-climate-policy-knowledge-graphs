pol:EU_32014L0094_Title_0_Chapter_0_Section_0_Article_06_TimeCompliance_1 a pol:Time_Compliance ;
    pol:Compliance_date "2025-12-31"^^xsd:date ;
    pol:annotatedText "Member States shall ensure that refuelling points for LNG at maritime ports are in place by 31 december 2025." .

pol:EU_32014L0094_Title_0_Chapter_0_Section_0_Article_06_TimeCompliance_2 a pol:Time_Compliance ;
    pol:Compliance_date "2030-12-31"^^xsd:date ;
    pol:annotatedText "Member States shall ensure that refuelling points for LNG at inland ports are in place by 31 december 2030." .

pol:EU_32014L0094_Title_0_Chapter_0_Section_0_Article_06_TimeCompliance_3 a pol:Time_Compliance ;
    pol:Compliance_date "2025-12-31"^^xsd:date ;
    pol:annotatedText "Member States shall ensure that refuelling points for LNG accessible to the public are in place by 31 december 2025." .

pol:EU_32014L0094_Title_0_Chapter_0_Section_0_Article_06_TimeCompliance_4 a pol:Time_Compliance ;
    pol:Compliance_date "2027-12-31"^^xsd:date ;
    pol:annotatedText "The Commission shall assess the application of the requirement and submit a proposal to amend this directive by 31 december 2027." .

pol:EU_32014L0094_Title_0_Chapter_0_Section_0_Article_06_TimeCompliance_5 a pol:Time_Compliance ;
    pol:Compliance_date "2020-12-31"^^xsd:date ;
    pol:annotatedText "Member States shall ensure that an appropriate number of CNG refuelling points are in place by 31 december 2020." .

pol:EU_32014L0094_Title_0_Chapter_0_Section_0_Article_06_TimeCompliance_6 a pol:Time_Compliance ;
    pol:Compliance_date "2025-12-31"^^xsd:date ;
    pol:annotatedText "Member States shall ensure that an appropriate number of CNG refuelling points along the existing TEN-T core network are in place by 31 december 2025." .

pol:EU_32014L0094_Title_0_Chapter_0_Section_0_Article_06_TimeCompliance_7 a pol:Time_Compliance ;
    pol:Compliance_date "2017-11-18"^^xsd:date ;
    pol:annotatedText "CNG refuelling points for motor vehicles deployed or renewed as from 18 november 2017 must comply with the technical specifications." .
