pol:32004L0008 a pol:PolicyDocument ;
    pol:hasArticle pol:EU_32004L0008_Title_0_Chapter_0_Section_0_Article_10 .

pol:EU_32004L0008_Title_0_Chapter_0_Section_0_Article_10 a pol:PolicyArticle ;
    pol:specifies_compliance_time pol:EU_32004L0008_Title_0_Chapter_0_Section_0_Article_10_TimeCompliance_1,
                                  pol:EU_32004L0008_Title_0_Chapter_0_Section_0_Article_10_TimeCompliance_2,
