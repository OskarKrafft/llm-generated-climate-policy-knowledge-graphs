    skos:definition "a company that sells energy to end-users"@en .

<polianno:Term12> a skos:Concept ;
    skos:prefLabel "Implementing Public Authority"@en ;
    skos:definition "an entity responsible for implementing public policies"@en .

<polianno:Term13> a skos:Concept ;
    skos:prefLabel "Individual Action"@en ;
    skos:definition "a single action taken by an individual"@en .

<polianno:Term14> a skos:Concept ;
    skos:prefLabel "Energy Management System"@en ;
    skos:definition "a system for managing energy consumption"@en .

<polianno:Term15> a skos:Concept ;
