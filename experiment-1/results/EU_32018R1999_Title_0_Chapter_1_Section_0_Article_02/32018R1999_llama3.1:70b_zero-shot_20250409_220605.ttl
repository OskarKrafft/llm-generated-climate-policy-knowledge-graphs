@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix padmin: <http://purl.org/net/po_admin/> .
@prefix polianno: <http://purl.org/net/polianne/> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .

<polianno:Article1> a owl:Ontology ;
    skos:definition "European Union article defining various energy-related terms"@en ;
    padmin:documentBaseURI <https://example.com/eu-article/> .

<polianno:Term1> a skos:Concept ;
    skos:prefLabel "Gas" ;
    skos:definition "a fossil fuel"@en .

<polianno:Term2> a skos:Concept ;
    skos:prefLabel "Obligated Party"@en ;
    skos:definition "an entity required to take certain actions"@en .

<polianno:Term3> a skos:Concept ;
    skos:prefLabel "Energy Efficiency Improvement"@en ;
    skos:definition "a measure to reduce energy consumption"@en .

<polianno:Term4> a skos:Concept ;
    skos:prefLabel "Cogeneration"@en ;
    skos:definition "the simultaneous production of heat and power"@en .

<polianno:Term5> a skos:Concept ;
    skos:prefLabel "Heat Pump"@en ;
    skos:definition "a device that uses refrigeration to transfer heat"@en .

<polianno:Term6> a skos:Concept ;
    skos:prefLabel "Nearly Zero-Energy Building"@en ;
    skos:definition "a building with very low energy consumption"@en .

<polianno:Term7> a skos:Concept ;
    skos:prefLabel "Building"@en ;
    skos:definition "a structure with walls and a roof"@en .

<polianno:Term8> a skos:Concept ;
    skos:prefLabel "Energy Performance Contracting"@en ;
    skos:definition "a contract to improve energy efficiency"@en .

<polianno:Term9> a skos:Concept ;
    skos:prefLabel "Distribution System Operator"@en ;
    skos:definition "an entity responsible for managing an energy distribution system"@en .

<polianno:Term10> a skos:Concept ;
    skos:prefLabel "Energy Service Provider"@en ;
    skos:definition "an entity that provides energy services"@en .

<polianno:Term11> a skos:Concept ;
