    skos:prefLabel "Fossil Fuel"@en ;
    skos:definition "non-renewable carbon-based energy sources"@en .

<polianno:Term17> a skos:Concept ;
    skos:prefLabel "Starch-Rich Crops"@en ;
    skos:definition "crops rich in starch"@en .

<polianno:Term18> a skos:Concept ;
    skos:prefLabel "Food and Feed Crops"@en ;
    skos:definition "crops used for food and animal feed"@en .

<polianno:Term19> a skos:Concept ;
    skos:prefLabel "Ligno-Cellulosic Material"@en ;
