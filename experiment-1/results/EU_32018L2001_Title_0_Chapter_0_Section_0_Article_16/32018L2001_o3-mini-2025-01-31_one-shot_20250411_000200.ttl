pol:32018L2001 a pol:PolicyDocument ;
    pol:hasArticle pol:EU_32018L2001_Title_0_Chapter_0_Section_0_Article_16 ;
    rdfs:comment "CELEX: 32018L2001; ELI: http://data.europa.eu/eli/dir/2018/2001/oj; Annotators: C, F." .

pol:EU_32018L2001_Title_0_Chapter_0_Section_0_Article_16 a pol:PolicyArticle ;
    rdfs:label "organisation and duration of the permit-granting process" ;
    pol:specifies_compliance_time pol:EU_32018L2001_Title_0_Chapter_0_Section_0_Article_16_TimeCompliance_4,
                                  pol:EU_32018L2001_Title_0_Chapter_0_Section_0_Article_16_TimeCompliance_5,
                                  pol:EU_32018L2001_Title_0_Chapter_0_Section_0_Article_16_TimeCompliance_6,
                                  pol:EU_32018L2001_Title_0_Chapter_0_Section_0_Article_16_TimeCompliance_8 .

pol:EU_32018L2001_Title_0_Chapter_0_Section_0_Article_16_TimeCompliance_4 a pol:Time_Compliance ;
    pol:annotatedText "Permit-granting process shall not exceed two years for power plants, extendable by up to one year." .

pol:EU_32018L2001_Title_0_Chapter_0_Section_0_Article_16_TimeCompliance_5 a pol:Time_Compliance ;
    pol:annotatedText "Permit-granting process shall not exceed one year for installations with an electrical capacity of less than 150 kW, extendable by up to one year." .

pol:EU_32018L2001_Title_0_Chapter_0_Section_0_Article_16_TimeCompliance_6 a pol:Time_Compliance ;
    pol:annotatedText "Repowering process shall not exceed one year, extendable by up to one year." .

pol:EU_32018L2001_Title_0_Chapter_0_Section_0_Article_16_TimeCompliance_8 a pol:Time_Compliance ;
    pol:annotatedText "The relevant authority shall decide within six months of receipt of a notification, where a simple-notification procedure is applied." .
