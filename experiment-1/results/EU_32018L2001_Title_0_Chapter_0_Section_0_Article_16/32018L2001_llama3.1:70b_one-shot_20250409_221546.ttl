    rdf:type poliana:Article ;
    rdfs:label "article 16" ;
    poliana:hasSection <http://data.europa.eu/eli/dir/2018/2001/oj#section_1>,
                       <http://data.europa.eu/eli/dir/2018/2001/oj#section_2>,
                       <http://data.europa.eu/eli/dir/2018/2001/oj#section_3>,
                       <http://data.europa.eu/eli/dir/2018/2001/oj#section_4>,
                       <http://data.europa.eu/eli/dir/2018/2001/oj#section_5>,
                       <http://data.europa.eu/eli/dir/2018/2001/oj#section_6>,
