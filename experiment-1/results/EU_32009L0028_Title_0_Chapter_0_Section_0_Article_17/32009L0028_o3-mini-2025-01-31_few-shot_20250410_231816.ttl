pol:32009L0028 a pol:PolicyDocument ;
    pol:hasArticle pol:EU_32009L0028_Title_0_Chapter_0_Section_0_Article_17 .

pol:EU_32009L0028_Title_0_Chapter_0_Section_0_Article_17 a pol:PolicyArticle ;
    pol:specifies_compliance_time pol:EU_32009L0028_Title_0_Chapter_0_Section_0_Article_17_TimeCompliance_1,
                                  pol:EU_32009L0028_Title_0_Chapter_0_Section_0_Article_17_TimeCompliance_2,
