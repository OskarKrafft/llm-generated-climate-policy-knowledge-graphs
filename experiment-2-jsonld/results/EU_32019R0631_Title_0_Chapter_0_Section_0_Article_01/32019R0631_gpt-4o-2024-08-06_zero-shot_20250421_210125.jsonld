      "type": "PolicyArticle",
      "label": {
        "value": "Article 1: Subject Matter and Objectives"
      },
      "is_about": [
        {
          "id": "Objective",
          "type": "skos:Concept",
          "prefLabel": "CO2 Emissions Performance Requirements"
        },
        {
          "id": "eurovoc:4060",
          "type": "Organization",
          "label": {
            "value": "European Union"
          }
        }
      ],
      "contains_objective": [
        {
          "id": "Objective_2020",
          "type": "skos:Concept",
          "prefLabel": "2020 CO2 Emissions Target",
          "comment": {
            "value": "EU fleet-wide target of 95 g CO2/km for new passenger cars and 147 g CO2/km for new light commercial vehicles."
          }
        },
        {
          "id": "Objective_2025",
          "type": "skos:Concept",
          "prefLabel": "2025 CO2 Emissions Target",
          "comment": {
            "value": "15% reduction of the 2021 target for new passenger cars and light commercial vehicles."
          }
        },
        {
          "id": "Objective_2030",
          "type": "skos:Concept",
          "prefLabel": "2030 CO2 Emissions Target",
          "comment": {
            "value": "37.5% reduction for passenger cars and 31% reduction for light commercial vehicles from the 2021 target."
          }
        }
      ],
      "specifies_time": [
        {
          "id": "Time_InEffect_2020",
          "type": "Time_InEffect",
          "In_effect_date": "2020-01-01"
        },
        {
          "id": "Time_InEffect_2025",
